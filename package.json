{
  "name": "vue-antd-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A modern vue admin panel built with Vue3, Ant Design Vue, Vite, TypeScript, and Monorepo. It's fast!",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "npm run lint:fix",
      "npm run format"
    ],
    "*.{css,scss,less,sass}": [
      "npm run format"
    ],
    "*.{md,json}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.24",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^24.10.1",
    "@types/nprogress": "^0.2.3",
    "@unocss/preset-attributify": "^66.5.10",
    "@unocss/preset-icons": "^66.5.10",
    "@unocss/preset-uno": "^66.5.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mockjs": "^1.1.0",
    "prettier": "^3.7.4",
    "sass": "^1.94.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "unocss": "^66.5.10",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-svg-component": "^0.12.4",
    "unplugin-vue-components": "^30.0.0",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-mock": "^3.0.2",
    "vue-tsc": "^3.1.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
